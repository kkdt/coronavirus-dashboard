{
  "manifest_version": 2,
  "name": "Coronavirus Dashboard",
  "author": "Thinh Ho",
  "version": "0.1",
  "description": "Coronavirus dashboard",
  "icons": {
    "16": "images/corona16.png",
    "48": "images/corona48.png",
    "128": "images/corona128.png"
  },
  "browser_action": {
    "default_title": "Coronavirus Dashboard"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": ["activeTab"]
}

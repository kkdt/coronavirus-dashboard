{
  "name": "coronavirus-dashboard-extension",
  "version": "0.1",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "pivotal-ui": "^19.2.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-layout": "^0.18.3",
    "react-scripts": "^3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "INLINE_RUNTIME_CHUNK=false react-scripts build",
    "build:dev": "NODE_ENV=dev INLINE_RUNTIME_CHUNK=false react-scripts build --debug ",
    "dist": "if [ -d build ]; then rm -rf dist/*.tar.gz && mkdir -p dist && cd build && tar czvf ../dist/coronavirus-dashboard-extension.tar.gz . && cd -; else echo '!!! No build detected !!!'; fi",
    "clean": "rm -rf build dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
